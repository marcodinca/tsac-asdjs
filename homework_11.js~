function orderIterativo(array)
{
	var pari = new Array();
	var dispari = new Array();
	var p=0;
	var d=0;
	for(var i=0;i<array.length;i++)
	{
		if(array[i]%2==0)
		{
			pari[p]=array[i];
		}
		else
		{
			dispari[d]=array[i];
		}
	}
	for(var i=0;i<array.length;i++)
	{
		for(int j=0;j<dispari.length;j++)	
		{
			array
		}
	}
}

var ris = orderIterativo([2,5,1,8]);
console.log(ris[0]);
console.log(ris[1]);
console.log(ris[2]);
console.log(ris[3]);